{

    "manifest_version": 2,
    "name": "TL;DR emails",
    "version": "1.0",
  
    "description": "Creates a TL;DR of your emails",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://www.bbc.co.uk/news/*"],
        "js": ["tldr.js"]
      }
    ]
  
  }